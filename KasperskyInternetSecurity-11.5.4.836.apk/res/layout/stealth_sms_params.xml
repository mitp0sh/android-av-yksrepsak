<?xml version="1.0" encoding="utf-8"?>
<ScrollView a:layout_width="fill_parent" a:layout_height="wrap_content"
  xmlns:a="http://schemas.android.com/apk/res/android">
    <LinearLayout a:orientation="vertical" a:layout_width="fill_parent" a:layout_height="wrap_content">
        <LinearLayout a:orientation="vertical" a:layout_width="fill_parent" a:layout_height="wrap_content">
            <TextView a:id="@id/TextView02" a:text="@string/str_send_command_enter_target_phone_prompt" style="?kis_dialog_message" />
            <EditText a:id="@id/SendCmdPhone" a:inputType="phone" style="?kis_dialog_edittext" />
        </LinearLayout>
        <LinearLayout a:orientation="vertical" a:layout_width="fill_parent" a:layout_height="wrap_content">
            <TextView a:id="@id/TextView03" a:text="@string/str_send_command_enter_code_prompt" style="?kis_dialog_message" />
            <EditText a:id="@id/SendCmdCode" a:digits="1,2,3,4,5,6,7,8,9,0" a:inputType="phone" a:imeOptions="actionDone" style="?kis_dialog_edittext" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>